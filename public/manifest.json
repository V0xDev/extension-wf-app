{
  "name": "wf-stat",
  "version": "1.0",
  "description": "Приложение статистики персонажа Warface",
  "manifest_version": 3,
  "icons": {
    "16": "./icon/icon-16.png",
    "48": "./icon/icon-48.png",
    "128": "./icon/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.warface.ru/*"]
}
